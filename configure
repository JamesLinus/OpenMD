     CFLAGS="-O2"
  fi

  if test -z "$CFLAGS"; then
	echo ""
	echo "********************************************************"
        echo "* WARNING: Don't know the best CFLAGS for this system  *"
        echo "* Use  make CFLAGS=..., or edit the top level Makefile *"
	echo "* (otherwise, a default of CFLAGS=-O will be used)     *"
	echo "********************************************************"
	echo ""
        CFLAGS="-O"
  fi



