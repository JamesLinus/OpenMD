  PREPFLAG=""
  case "${host_cpu}-${host_os}" in

  *linux*) if test "$F90" = ifc -o "$F90" = ifort; then
                    PREPFLAG="-fpp1 "
                fi;;
  *aix*)  if test "$F90" = xlf90 -o "$F90" = f90; then
                    PREPFLAG="-qsuffix=cpp=F90 "
