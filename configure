  *-cmdline\ * | *-ignore\ * | *-def\ *)
    ac_fc_v_output=`echo $ac_fc_v_output | sed "\
        s/-cmdline  *'[^']*'/ /g; s/-cmdline  *\"[^\"]*\"/ /g
