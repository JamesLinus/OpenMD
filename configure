  { { echo "$as_me:963: error: cannot find install-sh or install.sh in ac-tools $srcdir/ac-tools" >&5
echo "$as_me: error: cannot find install-sh or install.sh in ac-tools $srcdir/ac-tools" >&2;}
   { (exit 1); exit 1; }; }
